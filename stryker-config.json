{
  "mutate": [
    "src/Honua.Server.Core/**/*.cs",
    "src/Honua.Server.Host/**/*.cs",
    "!**/*Tests.cs",
    "!**/*.Designer.cs",
    "!**/Migrations/**/*.cs"
  ],
  "test-projects": [
    "tests/Honua.Server.Core.Tests/Honua.Server.Core.Tests.csproj"
  ],
  "reporters": [
    "html",
    "progress",
    "cleartext",
    "json"
  ],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "concurrency": 4,
  "dashboard-compare": true,
  "dashboard-api-key": "${STRYKER_DASHBOARD_API_KEY}",
  "baseline-enabled": true,
  "since": true,
  "diff-ignore-changes": [
    "**/*Tests.cs"
  ],
  "ignore-mutations": [
    "StringLiteral"
  ],
  "coverage-analysis": "perTest",
  "disable-bail": false
}