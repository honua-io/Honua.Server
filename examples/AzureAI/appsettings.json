{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "LlmProvider": {
    "Provider": "Azure",
    "FallbackProvider": "OpenAI",
    "DefaultTemperature": 0.2,
    "DefaultMaxTokens": 4000,
    "Azure": {
      "EndpointUrl": "https://YOUR-RESOURCE.openai.azure.com/",
      "ApiKey": "YOUR-API-KEY-HERE",
      "DeploymentName": "gpt-4-turbo",
      "EmbeddingDeploymentName": "text-embedding-3-large",
      "DefaultModel": "gpt-4o",
      "DefaultEmbeddingModel": "text-embedding-3-large"
    }
  },
  "AzureAISearch": {
    "Endpoint": "https://YOUR-SEARCH-SERVICE.search.windows.net",
    "ApiKey": "YOUR-SEARCH-API-KEY-HERE",
    "IndexName": "deployment-knowledge"
  },
  "ConnectionStrings": {
    "PostgreSQL": "Host=YOUR-POSTGRES-HOST.postgres.database.azure.com;Database=honua;Username=honuaadmin;Password=YOUR-PASSWORD;SSL Mode=Require"
  }
}
