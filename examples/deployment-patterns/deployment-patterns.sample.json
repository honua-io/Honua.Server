[
  {
    "id": "aws-zero-downtime",
    "name": "AWS Zero-Downtime Honua",
    "cloudProvider": "aws",
    "dataVolumeMin": 50,
    "dataVolumeMax": 200,
    "concurrentUsersMin": 50,
    "concurrentUsersMax": 300,
    "successRate": 0.97,
    "deploymentCount": 24,
    "configuration": {
      "database": {
        "engine": "Aurora PostgreSQL",
        "extensions": ["postgis", "pg_cron"],
        "highAvailability": true
      },
      "compute": {
        "ecsCluster": "honua-services",
        "autoscaling": {
          "min": 2,
          "max": 8
        }
      },
      "network": {
        "vpc": "prod-shared",
        "publicSubnetCount": 2,
        "privateSubnetCount": 2
      }
    }
  },
  {
    "id": "azure-scaled-analytics",
    "name": "Azure Scaled Analytics",
    "cloudProvider": "azure",
    "dataVolumeMin": 100,
    "dataVolumeMax": 600,
    "concurrentUsersMin": 80,
    "concurrentUsersMax": 500,
    "successRate": 0.95,
    "deploymentCount": 18,
    "configuration": {
      "database": {
        "service": "Azure Database for PostgreSQL",
        "tier": "BusinessCritical",
        "geoRedundantBackup": true
      },
      "compute": {
        "aksCluster": "honua-prod",
        "nodePools": [
          { "name": "services", "vmSize": "Standard_DS4_v2", "minCount": 3, "maxCount": 10 }
        ]
      },
      "observability": {
        "logAnalyticsWorkspace": "honua-monitor",
        "alertRules": ["api-latency", "queue-depth"]
      }
    }
  },
  {
    "id": "gcp-streaming-ingest",
    "name": "GCP Streaming Ingest",
    "cloudProvider": "gcp",
    "dataVolumeMin": 20,
    "dataVolumeMax": 120,
    "concurrentUsersMin": 30,
    "concurrentUsersMax": 150,
    "successRate": 0.92,
    "deploymentCount": 12,
    "configuration": {
      "database": {
        "service": "Cloud SQL for PostgreSQL",
        "replicas": 1,
        "extensions": ["postgis"]
      },
      "compute": {
        "gkeCluster": "honua-stream",
        "ingest": {
          "pubsub": true,
          "dataflow": {
            "template": "vector-tile-pipeline"
          }
        }
      },
      "storage": {
        "gcsBucket": "honua-tiles",
        "cache": "cloud-memorystore"
      }
    }
  }
]
