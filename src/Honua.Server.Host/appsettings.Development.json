{
  "ConnectionStrings": {
  },
  "honua": {
    "authentication": {
      "mode": "Local",
      "enforce": true,
      "quickStart": {
        "enabled": false
      }
    },
    "metadata": {
      "provider": "json",
      "path": "../../samples/metadata/sample-metadata.json"
    },
    "odata": {
      "enabled": true,
      "allowWrites": false,
      "defaultPageSize": 100,
      "maxPageSize": 1000,
      "emitWktShadowProperties": true
    }
  },
  "observability": {
    "logging": {
      "jsonConsole": true,
      "includeScopes": true
    },
    "metrics": {
      "enabled": true,
      "endpoint": "/metrics",
      "usePrometheus": true
    }
  },
  "Performance": {
    "OutputCache": {
      "MaxSizeMB": 256
    },
    "MemoryCache": {
      "MaxSizeMB": 512,
      "CompactionPercentage": 0.2,
      "ExpirationScanMinutes": 2
    }
  }
}
