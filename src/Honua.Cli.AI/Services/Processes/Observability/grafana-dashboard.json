{
  "dashboard": {
    "id": null,
    "uid": "honua-process-framework",
    "title": "Honua Process Framework - Production Monitoring",
    "tags": ["honua", "process-framework", "semantic-kernel"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "value": "Prometheus",
            "text": "Prometheus"
          }
        },
        {
          "name": "workflow_type",
          "type": "query",
          "datasource": "$datasource",
          "query": "label_values(process_started, workflow_type)",
          "multi": true,
          "includeAll": true,
          "current": {
            "value": "$__all",
            "text": "All"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Processes",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(process_active_count)",
            "legendFormat": "Active Processes"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Success Rate (24h)",
        "type": "gauge",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(process_completed[24h])) / (sum(increase(process_completed[24h])) + sum(increase(process_failed[24h]))) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Avg Execution Time",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "avg(process_execution_duration_milliseconds)",
            "legendFormat": "Avg Duration"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["mean"]
          },
          "textMode": "value_and_name",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 2
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate (1h)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(process_failed[1h]) * 100",
            "legendFormat": "Error Rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Process Starts Over Time",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (workflow_type) (rate(process_started{workflow_type=~\"$workflow_type\"}[5m]))",
            "legendFormat": "{{workflow_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "processes/sec"
          }
        }
      },
      {
        "id": 6,
        "title": "Process Completion Rate",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(process_completed[5m]))",
            "legendFormat": "Completed"
          },
          {
            "expr": "sum(rate(process_failed[5m]))",
            "legendFormat": "Failed"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "stacking": {
                "mode": "normal"
              }
            },
            "unit": "processes/sec"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Completed"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Failed"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "Execution Duration Percentiles",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(process_execution_duration_milliseconds_bucket[5m])) by (le))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(process_execution_duration_milliseconds_bucket[5m])) by (le))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(process_execution_duration_milliseconds_bucket[5m])) by (le))",
            "legendFormat": "P99"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Success Rate by Workflow Type",
        "type": "bargauge",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (workflow_type) (increase(process_completed{workflow_type=~\"$workflow_type\"}[24h])) / (sum by (workflow_type) (increase(process_completed{workflow_type=~\"$workflow_type\"}[24h])) + sum by (workflow_type) (increase(process_failed{workflow_type=~\"$workflow_type\"}[24h]))) * 100",
            "legendFormat": "{{workflow_type}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Step Execution Duration",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg by (step_name) (process_step_duration_milliseconds)",
            "legendFormat": "{{step_name}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "max", "last"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line"
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Step Failure Rate",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (step_name) (rate(process_step_failed[5m]))",
            "legendFormat": "{{step_name}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "failures/sec",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Infrastructure Health",
        "type": "stat",
        "gridPos": {"x": 0, "y": 28, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "up{job=\"process-framework\"}",
            "legendFormat": "Process Framework"
          },
          {
            "expr": "redis_up",
            "legendFormat": "Redis"
          },
          {
            "expr": "llm_service_available",
            "legendFormat": "LLM Service"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        }
      },
      {
        "id": 12,
        "title": "Active Processes by Workflow",
        "type": "piechart",
        "gridPos": {"x": 8, "y": 28, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (workflow_type) (process_active_count{workflow_type=~\"$workflow_type\"})",
            "legendFormat": "{{workflow_type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 13,
        "title": "Recent Errors",
        "type": "logs",
        "gridPos": {"x": 16, "y": 28, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "{job=\"process-framework\"} |= \"error\" or \"failed\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "$datasource",
          "enable": true,
          "expr": "changes(process_started[5m]) > 0",
          "iconColor": "blue",
          "tagKeys": "workflow_type"
        }
      ]
    }
  },
  "overwrite": true,
  "folderUid": "honua",
  "message": "Process Framework monitoring dashboard v1.0"
}
