{
  "logging": {
    "logLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Honua": "Debug",
      "Honua.Server.Core": "Debug"
    }
  },
  "allowedHosts": "*",
  "honua": {
    "metadata": {
      "provider": "json",
      "path": "metadata.json"
    },
    "authentication": {
      "mode": "QuickStart",
      "allowQuickStart": true
    },
    "odata": {
      "enabled": true,
      "allowWrites": true,
      "defaultPageSize": 100,
      "maxPageSize": 1000
    },
    "services": {
      "wfs": {
        "enabled": true
      },
      "wms": {
        "enabled": true
      },
      "wmts": {
        "enabled": true
      },
      "csw": {
        "enabled": true
      },
      "wcs": {
        "enabled": true
      },
      "stac": {
        "enabled": true
      }
    }
  }
}