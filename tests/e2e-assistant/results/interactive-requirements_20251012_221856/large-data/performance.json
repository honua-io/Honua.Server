{
  "cache": {
    "provider": "redis",
    "ttl": "1h",
    "maxMemory": "2gb",
    "evictionPolicy": "allkeys-lru",
    "compressionEnabled": true
  },
  "connectionPool": {
    "minConnections": 5,
    "maxConnections": 100,
    "connectionTimeout": "30s",
    "idleTimeout": "10m",
    "poolingEnabled": true
  },
  "database": {
    "enablePreparedStatements": true,
    "queryTimeout": "30s",
    "maxRetries": 3,
    "spatialIndexes": [
      {
        "table": "features",
        "column": "geometry",
        "method": "GIST"
      },
      {
        "table": "tiles",
        "column": "bounds",
        "method": "GIST"
      }
    ]
  },
  "responseCompression": {
    "enabled": true,
    "providers": [
      "gzip",
      "br"
    ],
    "mimeTypes": [
      "application/json",
      "application/geo\u002Bjson",
      "text/xml"
    ],
    "level": "Optimal"
  },
  "tilesOptimization": {
    "tileCache": true,
    "tileCacheTTL": "24h",
    "tileGenerationParallelism": 4,
    "vectorTileOptimization": true,
    "rasterTileCompression": "webp"
  }
}