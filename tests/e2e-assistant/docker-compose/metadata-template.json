{
  "catalog": {
    "id": "honua-ogc-sample",
    "title": "Honua OGC Sample Catalog",
    "description": "Sample metadata for OGC API Features E2E testing",
    "version": "2025.09",
    "links": [
      {
        "href": "https://ogcapi.ogc.org/features/",
        "rel": "about",
        "type": "text/html",
        "title": "OGC API Features"
      }
    ],
    "keywords": [
      "ogc",
      "features",
      "e2e-test"
    ]
  },
  "folders": [
    {
      "id": "transportation",
      "title": "Transportation",
      "order": 10
    }
  ],
  "dataSources": [
    {
      "id": "postgis-primary",
      "provider": "postgis",
      "connectionString": "Host=postgis;Database=honua;Username=honua_user;Password=honua_password"
    }
  ],
  "services": [
    {
      "id": "roads",
      "title": "Road Centerlines",
      "folderId": "transportation",
      "serviceType": "feature",
      "dataSourceId": "postgis-primary",
      "enabled": true,
      "description": "Transportation datasets for E2E testing",
      "keywords": [
        "transportation",
        "roads"
      ],
      "ogc": {
        "collectionsEnabled": true,
        "itemLimit": 1000,
        "defaultCrs": "EPSG:4326",
        "additionalCrs": [
          "EPSG:3857"
        ]
      }
    }
  ],
  "layers": [
    {
      "id": "roads-primary",
      "serviceId": "roads",
      "title": "Primary Roads",
      "description": "Major roadways for E2E testing",
      "geometryType": "Point",
      "idField": "road_id",
      "displayField": "name",
      "geometryField": "geom",
      "crs": [
        "EPSG:4326",
        "EPSG:3857"
      ],
      "keywords": [
        "roads",
        "transport"
      ],
      "storage": {
        "table": "roads_primary",
        "geometryColumn": "geom",
        "primaryKey": "road_id"
      }
    }
  ]
}
