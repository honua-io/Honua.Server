{
  "id": "honua-test-build",
  "version": "0.1.0",
  "name": "Honua Test Build",
  "description": "Simple test build using local Honua repository",
  "repositories": [
    {
      "name": "honua-local",
      "url": "file:///home/mike/projects/HonuaIO",
      "ref": "dev",
      "access": "public",
      "projects": [
        "src/Honua.Server.Core/Honua.Server.Core.csproj"
      ]
    }
  ],
  "modules": [
    "Honua.Server.Core"
  ],
  "targets": [
    {
      "id": "test-linux-x64",
      "provider": "on-premises",
      "compute": "x86_64",
      "architecture": "linux-x64",
      "tier": "test",
      "optimizations": {
        "pgo": false,
        "trim": false,
        "optimizationPreference": "speed"
      }
    }
  ],
  "deployment": {
    "environment": "test",
    "enableCache": false,
    "parallelism": 1,
    "resources": {
      "timeout": 30
    }
  },
  "optimizations": {
    "pgo": false,
    "trim": false
  }
}
