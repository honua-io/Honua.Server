# Azure Staging Environment Variables
# Copy to terraform.tfvars and customize

location        = "eastus"
container_image = "myregistry.azurecr.io/honua:staging"

# Custom domains (optional)
custom_domains = ["staging.api.honua.io"]

# CORS
cors_origins = [
  "https://staging.honua.io",
  "https://staging-app.honua.io"
]
