{
  "bindings": [
    {
      "name": "timerTrigger",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0 */90 * *",
      "runOnStartup": false
    },
    {
      "name": "httpTrigger",
      "type": "httpTrigger",
      "direction": "in",
      "methods": ["post"],
      "authLevel": "function"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    }
  ],
  "scriptFile": "index.js"
}
